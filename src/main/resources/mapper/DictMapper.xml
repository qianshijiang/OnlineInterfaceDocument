<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.interfacedocument.dao.dictDao">
  
   <!-- 通过用户Id查询 -->
   <select id="findById" resultType="com.cn.interfacedocument.entity.Dict">
      select t.* from DICT t where t.id = #{Id}
   </select>
   
   <!-- 通过登录名查询(支持邮箱,电话登录) -->
   <select id="findByName" resultType="com.cn.interfacedocument.entity.Dict">
      select t.* from worduser t where t.name = #{name} or t.email = #{name} or t.telephone = #{name}
   </select>
   
   <!-- 查询所有用户数据 -->
   <select id="findListDict" resultType="com.cn.interfacedocument.entity.Dict">
      select t.* from DICT t
      <where>
          <if test="id != null and id != ''">
             and t.id = #{id}
          </if>
          <if test="name != null and name != ''">
             and t.name = #{name}
          </if>
          <if test="code != null and code != ''">
             and t.code = #{code}
          </if>
          <if test="describes != null and describes != ''">
             and t.describes = #{describes}
          </if>
          <if test="type != null and type != ''">
             and t.type = #{type}
          </if>
          <if test="extend != null and extend != ''">
             and t.extend = #{extend}
          </if>
          <if test="showOrder != null">
             and t.showOrder = #{showOrder}
          </if>
      </where>
   </select>
   
