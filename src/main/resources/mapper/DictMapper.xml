   <!-- 单条插入数据 -->
   <insert id="insert" parameterType="com.cn.interfacedocument.entity.Dict">
       insert into DICT(
         ID,
         NAME,
         CODE,
         DESCRIBES,
         TYPE,
         EXTEND,
         SHOW_ORDER
       )(
         select 
         <choose>
            <when test="id != null">
             #{id}
            </when>
            <otherwise>
              NULL
            </otherwise>
         </choose>
         ,
         <choose>
            <when test="name != null and name != ''">
             #{name}
            </when>
            <otherwise>
              ''
            </otherwise>
           
         </choose>
           ,    
         <choose>
            <when test="code != null and code != ''">
             #{code}
            </when>
            <otherwise>
             ''
            </otherwise>
         </choose>
          ,
         <choose>
           <when test="describes != null and describes != ''">
             #{describes}
           </when>
           <otherwise>
             ''
           </otherwise>
         </choose>
         ,
         <choose>
           <when test="type != null and type !=''">
            #{type}
           </when>
           <otherwise>
            ''
           </otherwise>
         </choose>
         ,
         <choose>
           <when test="extend != null and extend != ''">
             #{extend}
           </when>
           <otherwise>
             ''
           </otherwise>
         </choose>
         ,
         <choose>
           <when test="showOrder != null">
             #{showOrder}
           </when>
           <otherwise>
             NULL
           </otherwise>
         </choose>
         from dual
       )
   </insert>
   
