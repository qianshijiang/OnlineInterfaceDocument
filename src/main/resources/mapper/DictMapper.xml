   <!-- 单条修改用户数据 -->
   <update id="update" parameterType="com.cn.interfacedocument.entity.Dict">
     update DICT set
       <if test="name != null and name != ''">
          NAME = #{name},
       </if>
       <if test="code != null and code != ''">
          CODE = #{code},
       </if>
       <if test="describes != null and describes != ''">
           DESCRIBES = #{describes},
       </if>
       <if test="type != null and type != ''">
           TYPE = #{type},
       </if>
       <if test="extend != null and extend != ''">
           EXTEND = #{extend},
       </if>
       <if test="showOrder != null and showOrder != ''">
           SHOW_ORDER = #{showOrder},
       </if> 
       where ID = #{id}
   </update>
   
